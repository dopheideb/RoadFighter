;   0x73F7 (call)
45a7_copy_to_vram:
	call 0053_SETWRT	;45a7 [BIOS]: Set VDP for writing, HL=VRAM address
	ld a,(00006h)		;45aa a = 0x98, i.e. get VDP's writing port #0.
	
	

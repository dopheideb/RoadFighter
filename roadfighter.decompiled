; We can get here from:
;   0x45E4 (call)
;   0x4610 (fall) HL changed
4611_copy_to_VRAM:
	call 04591h		;4611 4591_setwrt_wrapper(), C_shadow = 0x98 (VDP writing port #0)
	
	; We can get here from:
	;   0x462A (jump)
	;   0x4634 (jump)
	ld a,(de)		;4614 A = *(DE)
	and a			;4615 Set flags.
	ret z			;4616 if (A == 0) return, end of sequence.
	inc de			;4617 
	ld b,a			;4618 DJNZ: set counter
	and 07fh		;4619 A &= 0x7F (i.e. clear bit 7)
	cp b			;461b Compute A - B and set flags.
	jr z,$+16		;461c if (DE[0] & 0x80 == 0) jump to 0x462C
	and a			;461e Set flags.
	jr z,$-20		;461f if (DE[0] == 0x80) jump to 460b_hl=*de_and_inc_de_twice and restart 0x4611
	; Post: DE[0] >= 0x81
	ld b,a			;4621 B = A, set number of times to loop/djnz.
	
	4622_start_of_djnz_loop:
	ld a,(de)		;4622
	inc de			;4623
	exx			;4624
	out (c),a		;4625 Copy to VRAM
	exx			;4627
	djnz $-6		;4628 if (--b != 0) jump to 4622
	jr $-22			;462a jump to 4614
	
	; Copy the same byte B times to VRAM.
	; We can get here from:
	;   0x461C (jump)
	ld a,(de)		;462c
	inc de			;462d
	
	462e_start_of_djnz_loop:
	exx			;462e
	out (c),a		;462f Copy to VRAM
	exx			;4631
	djnz $-4		;4632 if (--b != 0) jump to 462e
	jr $-32			;4634 jump to 4614
	
	
	
4636_unknown:
	push de			;4636
	ld b,010h		;4637 b = 0x10
	
	4639_start_of_djnz_loop:
	call 04679h		;4639 4679_vram_copy_reverse_bits_to_vram_de
	inc hl			;463c
	inc e			;463d
	djnz $-5		;463e if (--b != 0) jump to 4639
	ld a,e			;4640
	sub 020h		;4641
	ld e,a			;4643
	bit 4,e			;4644
	jr z,$-15		;4646
	pop de			;4648
	ld a,020h		;4649
	call 04040h		;464b DE += a
	dec c			;464e
	jr nz,$-25		;464f if (?) jump to 4636
	ret			;4651



4652_something_with_vram_copy_with_reverse_bits:
	ld b,003h		;4652 b = 3
	push bc			;4654
	push hl			;4655
	push de			;4656
	ld b,008h		;4657 b = 8
	call 04679h		;4659 4679_vram_copy_reverse_bits_to_vram_de
	inc hl			;465c
	inc de			;465d
	djnz $-5		;465e
	dec c			;4660
	jr nz,$-10		;4661
	pop hl			;4663
	ld de,00800h		;4664
	add hl,de		;4667
	ex de,hl		;4668
	pop hl			;4669
	pop bc			;466a
	djnz $-23		;466b
	ret			;466d



466e_reverse_bits_of_register_a:
	push bc			;466e Save bc
	ld c,a			;466f c = a
	ld b,008h		;4670 b = 8
	
	; ?Is the carry zero here?
	
	; Assume c = 'abcdefgh', each letter an unknown bit.
	; Assume a = 'stuvwxyz', each letter an unknown bit.
	;         
	;         | After rr c                  | After rla
	; --------+-----------------------------+------------------
	; Round 0 | c = '*abcdefg', carry = 'h' | carry = 's', a = 'tuvwxyzh'
	; Round 1 | c = 's*abcdef', carry = 'g' | carry = 't', a = 'uvwxyzhg'
	; Round 2 | c = 'ts*abcde', carry = 'f' | carry = 'u', a = 'vwxyzhgf'
	; Round 3 | c = 'uts*abcd', carry = 'e' | carry = 'v', a = 'wxyzhgfe'
	; Round 4 | c = 'vuts*abc', carry = 'd' | carry = 'w', a = 'xyzhgfed'
	; Round 5 | c = 'wvuts*ab', carry = 'c' | carry = 'x', a = 'yzhgfedc'
	; Round 6 | c = 'xwvuts*a', carry = 'b' | carry = 'y', a = 'zhgfedcb'
	; Round 7 | c = 'yxwvuts*', carry = 'a' | carry = 'z', a = 'hgfedcba'
	;                                                           ^^^^^^^^
	;                                                           All bits reversed!
	4672_loop:
	rr c			;4672 Rotate right reg c via carry.
	rla			;4674 Rotate left  reg a via carry.
	djnz $-3		;4675 if (--b != 0) jump to 0x4672
	
	pop bc			;4677 Restore bc
	ret			;4678



; HL: VRAM source
; DE: VRAM destination
4679_vram_copy_reverse_bits_to_vram_de:
	call 004A_RDVRM		;4679 a = vram[hl]
	call 0466eh		;467c 466e_reverse_bits_of_register_a
	ex de,hl		;467f
	call 004D_WRTVRM	;4680 vram[hl] = a
	ex de,hl		;4683
	ret			;4684



	ld a,0b8h		;4685
	call 04bf6h		;4687
	ld a,01bh		;468a
	call 0407fh		;468c
	
	; Clear whole 0x4000 (16KiB) of VRAM.
	xor a			;468f a = 0
	ld h,a			;4690 h = 0
	ld l,a			;4691 l = 0
	ld bc,04000h		;4692 bc = 0x4000
	call 0056_FILVRM	;4695 hl = 0x0000 (vram address), bc = 0x4000
	
	ld hl,046a9h		;4698 hl = 0x46A9
	ld d,008h		;469b d = 8, loop 8 times
	ld c,000h		;469d c = 0
	
	; We are going to loop 8 times, starting with VDP register 0. 
	; This means we will write the following VDP registers:
	;
	; (Source: https://www.msx.org/wiki/Category:VDP_Registers) 
	;   VDP register 0 (d==8): Screen mode, video output,interruption line, color bus setting
	;   VDP register 1 (d==7): Screen mode, sprites size, Vblank, Display, VRAM mode setting
	;   VDP register 2 (d==6): Pattern layout table setting (Foreground)
	;   VDP register 3 (d==5): Colors table setting
	;   VDP register 4 (d==4): Patterns generator table setting
	;   VDP register 5 (d==3): Sprites attribute table setting
	;   VDP register 6 (d==2): VDP Table Base Address Registers
	;   VDP register 7 (d==1): Text and screen margin color setting
	;
	; We can get here from:
	;   0x469D (fall)      hl = 0x46A9
	;   0x46A6 (jump/loop) hl is increased by 1
	ld b,(hl)		;469f
	call 0047_WRTVDP	;46a0 B: data to write, C: Number of register (=0)
	inc hl			;46a3
	inc c			;46a4
	dec d			;46a5
	jr nz,$-7		;46a6 if (loop counter != 0) jump to 0x469F
	ret			;46a8
	
	; Data for VDP register #00.
	;   0x01 (0): disable external VDP input
	;   0x02 (1): M3=1: Graphics/Screen 2 Mode (since M1=0,M2=0)
	;   0x04 (0): Reserved, must be 0.
	;   0x08 (0): Reserved, must be 0.
	;   0x10 (0): Reserved, must be 0.
	;   0x20 (0): Reserved, must be 0.
	;   0x40 (0): Reserved, must be 0.
	;   0x80 (0): Reserved, must be 0.
	defb 0x02		;46a9
	
	; Data for VDP register #01, contains 8 VDP option control bits.
	;   0x01 (0): MAG0 sprites (1X)
	;   0x02 (1): Size 1 sprites (16 x 16 bit)
	;   0x04 (0): Reserved, must be 0.
	;   0x18 (00): M1=0,M2=0: Graphics/Screen 2 Mode (since M3=1)
	;   0x20 (1): Enables VDP interrupt.
	;   0x40 (1): Enable the active display.
	;   0x80 (1): Selects 16K bytes of VRAM (instead of 4K).
	defb 0xE2		;46aa
	
	; Data for VDP register #02, name table base address.
	;   0x0F (    1110): 0x0E * 0x400 == 0x3800
	;   0x10 (   0    ): Reserved, must be 0.
	;   0x20 (  0     ): Reserved, must be 0.
	;   0x40 ( 0      ): Reserved, must be 0.
	;   0x80 (0       ): Reserved, must be 0.
	defb 0x0E		;46ab
	
	; Data for VDP register #03, color table base address.
	;   Non screen 2 mode:
	;   0xFF (01111111): 0x7F * 0x40 == 0x1FC0
	;   
	;   Screen 2 mode (http://www.msxblog.es/wp-content/uploads/2009/09/vdpprogrammersguide.pdf):
	;   0x7F ( 1111111): Must be 1 in screen 2 mode.
	;   0x80 (0       ): Color table base address is 0x0000.
	defb 0x7F		;46ac
	
	; Data for VDP register #04, pattern generator base address.
	;   Non screen 2 mode:
	;   0x07 (     111): 0x07 * 0x800 == 0x3800
	;   0x08 (    0   ): Reserved, must be 0.
	;   0x10 (   0    ): Reserved, must be 0.
	;   0x20 (  0     ): Reserved, must be 0.
	;   0x40 ( 0      ): Reserved, must be 0.
	;   0x80 (0       ): Reserved, must be 0.
	;
	;   Screen 2 mode (http://www.msxblog.es/wp-content/uploads/2009/09/vdpprogrammersguide.pdf):
	;   0x07 (     111): Pattern generator base address is 0x2000.
	;   0xF8 (00000   ): Must be 0 in screen 2 mode.
	defb 0x07		;46ad
	
	; Data for VDP register #05, sprite attribute table base address.
	;   0x7F ( 1110110): 0x76 * 0x80 == 0x3B00
	;   0x80 (0       ): Reserved, must be 0.
	defb 0x76		;46ae
	
	; Data for VDP register #06, sprite pattern generator base address.
	;   0x03 (     011): 0x03 * 0x800 == 0x1800
	;   0x08 (    0   ): Reserved, must be 0.
	;   0x10 (   0    ): Reserved, must be 0.
	;   0x20 (  0     ): Reserved, must be 0.
	;   0x40 ( 0      ): Reserved, must be 0.
	;   0x80 (0       ): Reserved, must be 0.
	defb 0x03		;46af
	
	; Data for VDP register #07, text color.
	;   0x0F (    0100): backdrop color, 0x04 == dark blue
	;   0xF0 (1110    ): text color, 0x0E == gray
	defb 0xE4		;46b0



46b1_write_B_to_vdp_register_14:
	ld c,00eh		;46b1 c = 0x0E, VDP register to write to.
	jp 04038h		;46b3 4038_wrtvdp_hook, B: data to write, C: number of the register



; Read keyboard/joystick, and save it in 0xE009.
;
; Any keys that were previously unpressed but not pressed, are stored in 
; 0xE008.
; 
; We can get here from:
;   0x4023 (call)
46b6_read_and_save_joystick_and_keyboard:
	call 046c3h		;46b6 46c3_read_joystick_and_keyboard
	ld hl,0e009h		;46b9 hl = 0xE009
	
	; We can get here from:
	;   0x43C9 (call)
	;   0x46B9 (fall)
	ld c,(hl)		;46bc C = previous keyboard/joystick input
	ld (hl),a		;46bd Save new keyboard/joystick input
	xor c			;46be Compute all differences between previous and current input
	and (hl)		;46bf Select only differences in currently pressed input.
	dec hl			;46c0 HL = 0xE008
	ld (hl),a		;46c1 Save newly pressed keys
	ret			;46c2



; Output: register A, with the following bit meaning:
;   0x01 Status of "Up"
;   0x02 Status of "Down"
;   0x04 Status of "Left"
;   0x08 Status of "Right"
;   0x10 Status of "Space" / trigger 1
;   0x20 Status of "Select" / trigger 2
46c3_read_joystick_and_keyboard:	
	; PSG Register 0x0F (15):
	;   This register makes it possible to control the pins of the general ports to read 
	;   via register 14 as well as the state of the LED "code" or "kana" depending on the 
	;   type of keyboard.
	;
	; 0x8F:
	;   yes 0x80 (bit 7) LED control of the "Code" or "Kana" key. (1 to turn off)
	;   no  0x40 (bit 6) selection of the general port readable via register 14 (1 for port 2)
	;   no  0x20 (bit 5) pin control 8 of the general port 2 (0 for standard joystick mode)
	;   no  0x10 (bit 4) pin control 8 of the general port 1 (0 for standard joystick mode)
	;   yes 0x08 (bit 3) pin control 7 of the general port 2*
	;   yes 0x04 (bit 2) pin control 6 of the general port 2*
	;   yes 0x02 (bit 1) pin control 7 of the general port 1*
	;   yes 0x01 (bit 0) pin control 6 of the general port 1*
	;
	; Read joystick A, turn LED off.
	ld e,08fh		;46c3 e = 0x8F (data to write to PSG reg 15)
	ld a,00fh		;46c5 a = 0x0F (PSG reg)
	call 0093_WRTPSG	;46c7 A: PSG reg, E: data
	
	
	
	; PSG Register 0x0E (14):
	;
	; Bit 0 = Pin 1 state of the selected general port (Up if joystick)
	; Bit 1 = Pin 2 state of the selected general port (Down if joystick)
	; Bit 2 = Pin 3 state of the selected general port (Left if joystick)
	; Bit 3 = Pin 4 state of the selected general port (Right if joystick)
	;
	; Bit 4 = Pin 6 state of the selected general port (Trigger 1 if joystick)
	; Bit 5 = Pin 7 state of the selected general port (Trigger 2 if joystick)
	; Bit 6 = 1 for JIS, 0 for JP50on (only valid for Japanese MSX)
	; Bit 7 = CASRD (Reading signal on cassette)
	ld a,00eh		;46ca a = 0x0E
	di			;46cc
	call 0096_RDPSG		;46cd The value read is stored in register A.
	ei			;46d0
	cpl			;46d1 Flip all bits (because joystick port is active low).
	and 03fh		;46d2 up, down, left, right, trigger 1, trigger 2.
	push af			;46d4 Save joystick state.
	
	
	
	;;;
	; Read keyboard.
	;;;
	
	; Note: the player can use both keyboard and joystick A at the 
	; same time. The keys read from the keyboard are therefore 
	; mapped to the same bits as the joystick, so we can OR them at 
	; the end.
	
	; Get status of "Select" key.
	; 
	; Line 7:
	; 0x80   0x40   0x20      0x10 0x08 0x04   0x02 0x01
	; Return Select	Backspace Stop Tab  Escape F5   F4
	;        ?End?
	ld a,007h		;46d5 a = 7, the line of the keyboard matrix to get.
	call 0141_SNSMAT	;46d7 Get specified line from the keyboard matrix.
	cpl			;46da Flip all bits (because keyboard matrix is active low).
	rrca			;46db rotate right, bit 0 to both bit 7 and carry.
	and 020h		;46dc a &= 0x20 ("Select" key)
	ld e,a			;46de e = a
	
	
	
	; Get status of spacebar and arrow keys.
	;
	; Line 8:
	; 0x80 0x40 0x20 0x10 0x08 0x04 0x02 0x01
	;        |    ^
	; --->   |    |  <---  DEL  INS HOME SPACE
	;        V    |
	ld a,008h		;46df a = 8, the line of the keyboard matrix to get.
	call 0141_SNSMAT	;46e1 Get specified line from the keyboard matrix.
	cpl			;46e4 Flip all bits (because keyboard matrix is active low).
	rrca			;46e5 rotate right: space right down up left del ins home
	rrca			;46e6 rotate right: home space right down up left del ins
	ld b,a			;46e7 Save
	and 004h		;46e8 a &= 0x04, get status of "Left" key
	or e			;46ea a |= e, add status of "Select" key (0x20 or 0x00)
	ld c,a			;46eb
	; Post: Bit meaning of register C:
	;   0x04 Status of "Left" key
	;   0x20 Status of "Select" key
	
	ld a,b			;46ec home space right down up left del ins
	rrca			;46ed ins home space left down up right del
	rrca			;46ee del ins home space right down up left
	ld b,a			;46ef Save
	and 018h		;46f0 Get status of "Right" key and "Space" key
	or c			;46f2 Add status of "Left" and "Select" key
	ld c,a			;46f3 Save
	; Post: Bit meaning of register C:
	;   0x04 Status of "Left" key
	;   0x08 Status of "Right" key
	;   0x10 Status of "Space" key
	;   0x20 Status of "Select" key
	
	ld a,b			;46f4 del ins home space right down up left
	rrca			;46f5 left del ins home space right down up
	and 003h		;46f6 Get status of "Down" key and "Up" key
	or c			;46f8 Add status of "Left"+"Right"+"Space"+"Select"
	; Post: Bit meaning of register A:
	;   0x01 Status of "Up" key
	;   0x02 Status of "Down" key
	;   0x04 Status of "Left" key
	;   0x08 Status of "Right" key
	;   0x10 Status of "Space" key
	;   0x20 Status of "Select" key
	
	pop bc			;46f9 b = joystick A state (see push at 0x46D4)
	or b			;46fa Combine input from keyboard and joystick
	; Post: Bit meaning of register A:
	;   0x01 Status of "Up"
	;   0x02 Status of "Down"
	;   0x04 Status of "Left"
	;   0x08 Status of "Right"
	;   0x10 Status of "Space" / trigger 1
	;   0x20 Status of "Select" / trigger 2
	
	ret			;46fb




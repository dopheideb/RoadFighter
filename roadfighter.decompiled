	; ?Is the carry zero here?
	
	; Assume c = 'abcdefgh', each letter an unknown bit.
	; Assume a = 'stuvwxyz', each letter an unknown bit.
	;         
	;         | After rr c                  | After rla
	; --------+-----------------------------+------------------
	; Round 0 | c = '*abcdefg', carry = 'h' | carry = 's', a = 'tuvwxyzh'
	; Round 1 | c = 's*abcdef', carry = 'g' | carry = 't', a = 'uvwxyzhg'
	; Round 2 | c = 'ts*abcde', carry = 'f' | carry = 'u', a = 'vwxyzhgf'
	; Round 3 | c = 'uts*abcd', carry = 'e' | carry = 'v', a = 'wxyzhgfe'
	; Round 4 | c = 'vuts*abc', carry = 'd' | carry = 'w', a = 'xyzhgfed'
	; Round 5 | c = 'wvuts*ab', carry = 'c' | carry = 'x', a = 'yzhgfedc'
	; Round 6 | c = 'xwvuts*a', carry = 'b' | carry = 'y', a = 'zhgfedcb'
	; Round 7 | c = 'yxwvuts*', carry = 'a' | carry = 'z', a = 'hgfedcba'
	;                                                           ^^^^^^^^
	;                                                           All bits reversed!
	4672_loop:
	rr c			;4672 Rotate right reg c via carry.
	rla			;4674 Rotate left  reg a via carry.
	djnz $-3		;4675 if (--b != 0) jump to 0x4672
	
	pop bc			;4677 Restore bc
	ret			;4678




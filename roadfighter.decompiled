; z80dasm 1.1.6
; command line: z80dasm --address --origin=16384 --output=roadfighter.decompiled.org RoadFighter.rom

	; C-BIOS MSX1
	org	00000h
	(...)
	jp 0022eh		;0047 [BIOS] WRTVDP: Write to any VDP register
	jp 00245h		;004a [BIOS] RDVRM : Read byte from VRAM
	jp 0024dh		;004d [BIOS] WRTVRM: Write byte to VRAM
	jp 00255h		;0050 [BIOS] SETRD : Set up VDP for read
	jp 00260h		;0053 [BIOS] SETWRT: Set up VDP for write
	jp 0026dh		;0056 [BIOS] FILVRM: Fill block of VRAM with data byte
	(...)
	jp 01752h		;0093 [BIOS] WRTPSG: Write to any PSG register
	jp 0175ch		;0096 [BIOS] RDPSG : Read from any PSG register
	(...)
	jp 01770h		;013e [BIOS] RDVDP : Read VDP Status Register
	jp 01773h		;0141 [BIOS] SNSMAT: Read row of keyboard matrix
	(...)
	
0260_SETWRT:
	di			;0260 Disable interrupts.
	
	; 0x99: Writing port 1: VRAM address and register set-up.
	ld a,l			;0261 A = L
	out (099h),a		;0262 Store low byte of VRAM address to write to.
	ld a,h			;0264 A =  H
	; Bit 7 is unused and must be 0.
	; Bit 6:
	;   0: Set-up for READING from VRAM.
	;   1: Set-up for WRIING to VRAM.
	and 03fh		;0265 A = (H & 0x3F), bit 6 and 7 are not part of the VRAM address
	or 040h			;0267 A = (H & 0x3F) | 0x40, we want to WRITE to VRAM.
	out (099h),a		;0269 Store high byte of VRAM address to write to.
	
	ei			;026b Enable interrupts.
	ret			;026c



	; Road Fighter ROM	
	org	04000h

	defm "AB"		;4000 ID. Must be "AB" for cartridge.
	defw 0x404F		;4002 INIT. Entry point.
	defw 0x0000		;4004 STATEMENT.                     Not used, so zeroed.
	defw 0x0000		;4006 DEVICE.                        Not used, so zeroed.
	defw 0x0000		;4008 TEXT.                          Not used, so zeroed.
	defw 0x0000		;400A RESERVED FOR FUTURE USE.       Not used, so zeroed.
	defw 0x0000		;400C RESERVED FOR FUTURE USE.       Not used, so zeroed.
	defw 0x0000		;400E RESERVED FOR FUTURE USE.       Not used, so zeroed.

4010_unknown_hook:
	jp 048c9h		;4010

4013_vdp_hook:
	call 013E_RDVDP		;4013 Read VDP status register; clears interrupt.
	di			;4016 Disable interrupts.
	call 04bffh		;4017
	ld hl,0e005h		;401a hl = 0xE005
	bit 0,(hl)		;401d
	jr nz,$+15		;401f if (nz) jump to 0x402E
	inc (hl)		;4021
	ei			;4022 Enable interrupts.
	call 046b6h		;4023 46b6_read_and_save_joystick_and_keyboard
	call 04082h		;4026
	ld a,000h		;4029 a = 0
	ld (0e005h),a		;402b *(0xE005) = 0
	
	; We can get here from:
	;   0x401F (jump)
	;   0x402B (fall)
	call 013E_RDVDP		;402e Read VDP status register; clears interrupt.
	or a			;4031
	di			;4032 Disable interrupts.
	call m,04bffh		;4033
	ei			;4036 Enable interrupts.
	ret			;4037



4038_wrtvdp_hook:
	jp 0047_WRTVDP		;4038



; Computes HL += a
403b_HL+=a:
	add a,l			;403b
	ld l,a			;403c
	ret nc			;403d
	inc h			;403e
	ret			;403f



; Computes DE += a
4040_DE+=a:
	add a,e			;4040
	ld e,a			;4041
	ret nc			;4042
	inc d			;4043
	ret			;4044



; Computes pc += 2 * a (the PC from the caller perspective)
; 
; Destroys A, DE, HL
4045_pc=pc[2*a]:
	add a,a			;4045 a += a
	
	; Z80 pushed the program counter (pc) when this subroutine was 
	; called. (Actually the PC right after the call.) So, that PC is 
	; popped here.
	pop hl			;4046 hl = *(sp)
	
	call 0403bh		;4047 hl += a
	ld e,(hl)		;404a e = *(hl)
	inc hl			;404b ++hl
	ld d,(hl)		;404c d = *(hl)
	ex de,hl		;404d de=hl, hl=de
	jp (hl)			;404e



entry_point:
	di			;404f Disable interrupts.
	im 1			;4050 Set the interrupt mode.
setup_vdp_hook:
	; Set H.KEYI to "jp 0x4013"
	ld a,0c3h		;4052 a = 0xC3, which is "jp"
	ld (0fd9ah),a		;4054 "JP"
	ld hl,04013h		;4057 hl = 0x4013
	ld (0fd9bh),hl		;405a
	
	ld sp,0e800h		;405d Set top of stack.
	
	; Clear variables.
	ld hl,e000_game_state	;4060 LDIR: source
	ld de,e001_game_substate;4063 LDIR: destination
	ld bc,007ffh		;4066 LDIR: count (0x800)
	ld (hl),000h		;4069
	ldir			;406b

	ld a,001h		;406d a = 1
	ld (0e005h),a		;406f *(0xE005) = 1
	call 04685h		;4072

	xor a			;4075 a = 0
	ld (0e005h),a		;4076 *(0xE005) = 0
	call 013E_RDVDP		;4079 Read VDP status register; clears interrupt.
	ei			;407c Enable interrupts
	; End of entry point. Interrupt and 0x4013 take over. (Main does 
	; nothing.)



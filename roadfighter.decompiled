; 
; Input:
;   BC: Number of bytes to copy
;   DE: RAM address
;   HL: VRAM address
; We can get here from:
;   0x42AD (call)
;   0x4507 (call)
;   0x4578 (call)
;   0x45c2 (call)
;   0x5279 (call)
;   0x6B4A (call)
;   0x73CA (call)
;   0x73E6 (call)
;   0x73F7 (call)
45a7_copy_to_vram:
	call 0053_SETWRT	;45a7 [BIOS]: Set VDP for writing, HL=VRAM address
	ld a,(00006h)		;45aa a = 0x98, i.e. get VDP's writing port #0.
	
	
	; All registers are used, so use a shadow register (c to be 
	; precise) to store the VDP writing port #0.
	exx			;45ad
	ld c,a			;45ae c = 0x98
	exx			;45af
	
	ld a,(de)		;45b0 a = *(de)
	exx			;45b1
	out (c),a		;45b2 Write *(de) to VDP's writing port.
	exx			;45b4
	inc de			;45b5 Increase source RAM address.
	; Note: destination VRAM address is auto incremented by the VDP.
	dec bc			;45b6 Decrement byte counter.
	ld a,b			;45b7
	or c			;45b8
	jr nz,$-9		;45b9 if (bc != 0) jump to 45b0
	ret			;45bb




; Computes pc += 2 * a (the PC from the caller perspective)
; 
; Destroys A, DE, HL
4045_pc=pc[2*a]:
	add a,a			;4045 a += a
	
	; Z80 pushed the program counter (pc) when this subroutine was 
	; called. (Actually the PC right after the call.) So, that PC is 
	; popped here.
	pop hl			;4046 hl = *(sp)
	
	call 0403bh		;4047 hl += a
	ld e,(hl)		;404a e = *(hl)
	inc hl			;404b ++hl
	ld d,(hl)		;404c d = *(hl)
	ex de,hl		;404d de=hl, hl=de
	jp (hl)			;404e




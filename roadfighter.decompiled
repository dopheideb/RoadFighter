; Register A: tune ID
;
; We can get here from
;   0x4B71 (call) D=0
;   0x4BD8 (call) D=1
4b76_sound_something:
	ld c,a			;4b76 C = tune ID
	ld b,001h		;4b77 B = 1, default number of channels
	ld hl,0e012h		;4b79 HL = 0xE012
	and 03fh		;4b7c
	cp 001h			;4b7e
	jr z,$+35		;4b80 if (tune_ID & 0x3F == 0) jump to 0x4BA3
	cp 005h			;4b82
	jr c,$+16		;4b84 if (tune_ID & 0x3F < 5) jump to 0x4B94
	cp 00fh			;4b86
	jr c,$+6		;4b88 if (tune_ID & 0x3F < 0xF) jump to 0x4B8E
	inc b			;4b8a B = 2
	inc b			;4b8b B = 3
	jr $+11			;4b8c jump to 0x4B97
	
	; We can get here from:
	;   0x4B88 (jump) tuneID & 0x3F in [5,6,7,8,9,0xA,0xB,0xC,0xD,0xE]
	4b8e_:
	ld hl,0e01dh		;4b8e HL = 0xE01D
	inc b			;4b91 B = 2
	jr $+5			;4b92 jump to 0x4b97
	
	; We can get here from:
